{
	"name": "innoweb/silverstripe-email-obfuscator",
	"description": "Obfuscates all email addresses displayed on the site. Email addresses are revealed using Javascript.",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "email", "obfuscate"],
	"homepage": "https://github.com/xini/silverstripe-email-obfuscator",
	"support": {
		"issues": "https://github.com/xini/silverstripe-email-obfuscator/issues"
	},
	"authors": [{
		"name": "Florian Thoma",
		"homepage": "https://innoweb.com.au"
	}],
	"require": {
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/cms": "^4"
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\EmailObfuscator\\": "src/"
		}
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
	}
}
